[Unit]
Description=A broker to add new wireguard keys to the network
Wants=network.target
After=network.target

[Service]
User=auto
WorkingDirectory=/home/auto
Environment="PYTHONUNBUFFERED=TRUE"
ExecStart=/home/auto/.local/bin/gunicorn -b [::]:8000 broker:app
Restart=always

[Install]
WantedBy=multi-user.target
